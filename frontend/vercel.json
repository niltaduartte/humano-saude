{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/social-flow/cron?job=publish",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/social-flow/cron?job=analytics",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/social-flow/cron?job=tokens",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/social-flow/cron?job=sync",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/audit-campaigns",
      "schedule": "*/30 * * * *"
    }
  ]
}
